    <script>
        function zeroPadded(val) {
            if (val >= 10)
                return val;
            else
                return '0' + val;
        }
        function fixDate(input_date) {
            var value = new Date
                (
                    parseInt(input_date.replace(/(^.*\()|([+-].*$)/g, ''))
                );
            var output_date = value.getFullYear() +
                "-" +
                zeroPadded(value.getMonth() + 1) +
                "-" +
                zeroPadded(value.getDate());
            return output_date;
        }
        function fixDateTimeLocal(input_date) {

            var value = new Date
                (
                    parseInt(input_date.replace(/(^.*\()|([+-].*$)/g, ''))
                );
            var output_date = value.getFullYear() +
                "-" +
                zeroPadded(value.getMonth() + 1) +
                "-" +
                zeroPadded(value.getDate())
                + "T"
                + zeroPadded(value.getHours())
                + ":"
                + zeroPadded(value.getMinutes());
            return output_date;
        }

        function fixTime(input_time) {

            var Hours = null;
            if (input_time.Hours < 10)
                Hours = "0" + input_time.Hours;
            else
                Hours = input_time.Hours;

            var Minutes = null;
            if (input_time.Minutes < 10)
                Minutes = "0" + input_time.Minutes;
            else
                Minutes = input_time.Minutes;
            var output_time = Hours + ":" + Minutes;
            return output_time;
        }
        $(document).ready(function () {
            App.init();
            

            

            
        });
    </script>
